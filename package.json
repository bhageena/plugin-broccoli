{
  "name": "@techlode/plugin-broccoli1",
  "description": "oclif plugin",
  "version": "0.0.6",
  "author": "Chandan Rai @bhageena",
  "bugs": {
    "url": "https://github.com/bhageena/broccoli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "globby": "^11.0.0",
    "tslib": "^1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/bhageena/broccoli",
  "keywords": [
